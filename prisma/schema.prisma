
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


enum Role {
  SUPERADMIN
  ADMIN
  USER
}
model User {
  id Int @id @default(autoincrement())
  name String
  email String @unique
  profileImage String?
  phone String?
  password String
  role Role @default(USER)
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  @@map("users")
}

